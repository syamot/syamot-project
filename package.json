  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "dotenv": "^16.1.4",
    "emailjs-com": "^3.2.0",
    "express": "^4.18.2",
    "js-sha512": "^0.8.0",
    "knex": "^2.4.2",
    "nodemon": "^2.0.22",
    "pg": "^8.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
